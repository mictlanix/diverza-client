<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://www.buzonfiscal.com/ns/referenceID/v1" elementFormDefault="qualified" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.buzonfiscal.com/ns/referenceID/v1" attributeFormDefault="unqualified">
	<xsd:simpleType name="refIDType">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1" />
			<xsd:maxLength value="32" />
			<xsd:pattern value="\w+" />
			<xsd:whiteSpace value="collapse"/>
		</xsd:restriction>
	</xsd:simpleType>

    <xsd:element name="ReferenceID" type="refIDType"></xsd:element>
</xsd:schema>